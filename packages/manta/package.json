{
  "name": "manta-cli",
  "version": "0.1.4",
  "description": "Manta CLI: provider proxy + job worker with Codex and MCP integration",
  "type": "module",
  "private": true,
  "bin": {
    "manta": "dist/bin.js"
  },
  "oclif": {
    "bin": "manta",
    "commands": "./dist/commands"
  },
  "scripts": {
    "build": "tsc -p .",
    "pack:editor": "node ./scripts/pack-editor.mjs",
    "dev": "npm run build && node dist/bin.js",
    "start": "node dist/bin.js",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "license": "MIT",
  "dependencies": {
    "@oclif/core": "^3.26.6",
    "jszip": "^3.10.1",
    "execa": "^9.6.0"
  },
  "devDependencies": {
    "typescript": "^5.5.4"
  }
}
