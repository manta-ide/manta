{
  "name": "manta-ide",
  "version": "0.1.1",
  "private": false,
  "type": "module",
  "bin": {
    "manta": "packages/manta/dist/bin.js"
  },
  "oclif": {
    "bin": "manta",
    "commands": "./packages/manta/dist/commands"
  },
  "scripts": {
    "i:all": "npm install && cd packages/manta-mcp && npm install && cd ../manta && npm install",
    "mcp:inspect": "npx @modelcontextprotocol/inspector node packages/manta-mcp/dist/index.js",
    "mcp": "node packages/manta-mcp/dist/index.js",
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "dev:app": "cd vite-base-template && npm run dev -- --port 3001",
    "build:app": "cd base-template && npm run build",
    "start:app": "cd base-template && npm run start -- --port 3001",
    "dev:both": "concurrently \"npm run dev\" \"npm run dev:app\"",
    "start:both": "concurrently \"npm run start\" \"npm run start:app\"",
    "build:cli": "npm --prefix packages/shared-schemas run build && npm --prefix packages/manta-mcp run build && npm --prefix packages/manta run build",
    "pack:editor": "npm --prefix packages/manta run pack:editor",
    "build:all": "npm run build && npm run build:cli && npm run pack:editor",
    "prepublishOnly": "npm run build:all"
  },
  "dependencies": {
    "@headless-tree/core": "^1.2.1",
    "@headless-tree/react": "^1.2.1",
    "@internationalized/date": "^3.8.2",
    "@modelcontextprotocol/sdk": "^1.17.5",
    "@oclif/core": "^3.26.6",
    "@origin-space/image-cropper": "^0.1.9",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-slot": "^1.2.3",
    "@remixicon/react": "^4.6.0",
    "@types/diff-match-patch": "^1.0.36",
    "@types/jszip": "^3.4.0",
    "@types/pg": "^8.15.5",
    "@xyflow/react": "^12.8.3",
    "ai": "^4.3.19",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "diff-match-patch": "^1.0.5",
    "elkjs": "^0.10.0",
    "fast-xml-parser": "^5.2.5",
    "json-schema-to-zod": "^2.6.1",
    "jszip": "^3.10.1",
    "lucide-react": "^0.525.0",
    "motion": "^12.23.12",
    "next": "^15.5.2",
    "next-themes": "^0.4.6",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "radix-ui": "^1.4.2",
    "react": "19.1.0",
    "react-aria-components": "^1.10.1",
    "react-colorful": "^5.6.1",
    "react-day-picker": "^9.8.0",
    "react-dom": "19.1.0",
    "react-markdown": "^10.1.0",
    "react-resizable-panels": "^3.0.4",
    "recharts": "^2.15.4",
    "remark-gfm": "^4.0.1",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.3.1",
    "web-worker": "^1.5.0",
    "zod": "^3.25.76",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/body-parser": "^1.19.6",
    "@types/express": "^5.0.3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.3.5",
    "typescript": "^5"
  },
  "optionalDependencies": {
    "manta-mcp": "^0.1.2"
  },
  "files": [
    "packages/manta/dist",
    "README.md"
  ]
}
