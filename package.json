  "scripts": {
    "dev": "concurrently \"npm run dev:ide\" \"npm run dev:app\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "dev:ide": "next dev --turbopack",
    "dev:app": "cd test-project && npm run dev -- --port 3001",
    "build:app": "cd test-project && npm run build",
    "start:app": "cd test-project && npm run preview -- --port 3001",
    "start:both": "concurrently \"npm run start\" \"npm run start:app\""
  },
