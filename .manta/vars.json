{
  "header-styles": {
    "background-color": "#27272a",
    "border-color": "#3f3f46",
    "border-width": 1,
    "padding-x": 16,
    "padding-y": 6
  },
  "layout-settings": {
    "justify-content": "space-between",
    "align-items": "center",
    "gap": 12
  },
  "component-behavior": {
    "fixed-position": false,
    "sticky": false,
    "z-index": 10
  },
  "node-background-color": "#ffffff",
  "selected-background-color": "#f8fafc",
  "border-color": "#e5e7eb",
